    <style>
        * {
            font-family: 'Roboto', sans-serif;
            margin: 0;
            padding: 0;
            font-family: 'Allura', cursive;
            font-family: 'Open Sans', sans-serif;
            font-family: 'Varela Round', sans-serif;
        }
        
        .container{
            display: flex;
            flex-direction: row;
            justify-content: center;
            align-items: center;
            float: left;
        }
        body{
            width: 100%;
            height: 100vh;
            background-color: black;
            scroll-behavior: smooth;
            -ms-overflow-style: none; /* for Internet Explorer, Edge */
            scrollbar-width: none; /* for Firefox */
            
        }
        body::-webkit-scrollbar {
            display: none; /* for Chrome, Safari, and Opera */
        }
        
        .navbar-1{
            display: flex;
            flex-direction: row;
            justify-content: space-between;
            align-items: center;
            width: 100%;
            height: 70px;
            background-color: black;
            color: white;
        }
        span{
            font-weight: lighter;
            font-size: 30px;
            margin-left: 10%;
        }
        .main{
            display: flex;
            flex-direction: row;
            justify-content: center;
            margin-right: 5%;
        }
        .main li {
            margin: 0px 20px;
            font-size: 21px;
            list-style-type: none;
            line-height: 70%;
        }
        .main li a {
            color: white;
            text-decoration: none;
        }
        .header{
            display: flex;
            flex-direction: row;
            justify-content: space-evenly;
            align-items: center;
            width: 100%;
            height: 100vh;
        }
        .pic{
            color: white;
            width: 500px;
            height: 500px;
        }
        .button {
           background: rgb(78,12,92);
        background: linear-gradient(153deg, rgba(78,12,92,1) 18%, rgba(188,8,255,1) 67%, rgba(150,57,156,1) 100%);
            padding: 20px 30px;
            color: white;
            text-decoration: none;
            font-size: 20px;
            border-radius: 20px;
        }
        .courses {
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            height: 100vh;
        }
        
        .component {
            background: rgb(3,16,38);
            background:  transparent linear-gradient(153deg, rgba(78,12,92,1) 18%, rgba(188,8,255,1) 67%, rgba(150,57,156,1) 100%) repeat scroll 0 0;
            width: 90%;
            height: 60px;
            text-align: center;
            color: white;
            line-height: 60px;
            border-radius:20px ;
        }
        .data{
            display: flex;
            flex-direction: row;
            justify-content: center;
            align-items: center;
            flex-wrap: wrap;
            width: 100%;
        }
        .box1{
            width: 300px;
            height: 200px;
            border-radius:20px ;
            background: rgb(78,12,92);
            background: linear-gradient(153deg, rgba(78,12,92,1) 18%, rgba(188,8,255,1) 67%, rgba(150,57,156,1) 100%);
            margin: 30px;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
        }
        
        i{
            color: white;
            font-size: 40px;
        }
        .content{
            color: white;
            font-size: 20px;
            margin: 10px;
            text-decoration: none;
        }
        
        .about-1{
            width: 100%;
            height: 100vh;
            display: flex;
            flex-direction: row;
            flex-wrap: nowrap;
            justify-content: center;
            align-items: center;
            width: 100%;
        }
        
        
        .age {
            font-size: 200px;
            color: white;
        }
        .old{
            color: white;
            font-size: 19px;
            margin-left: 0%;
            margin-top: 0%;
        }
        .name {
            color: rgba(188,8,255,1);
            font-size: 40px;
            margin: 30px;
        }
        
        .part-1 {
            display: flex;
            flex-direction: row;
            flex-wrap: wrap;
            justify-content: center;
            align-items: center;
            width: 50%;
        }
        
        .query{
            color: white;
            font-weight: lighter;
        }
        .part-2{
            display: flex;
            flex-direction: column;
            flex-wrap: wrap;
            justify-content: start;
            align-items: center;
            width: 50%;
        
        }
        .social{
            display: flex;
            flex-direction: row;
            flex-wrap: nowrap;
            justify-content: center;
            align-items: center;
        }
        
        .social a i {
            color: rgba(188,8,255,1);
            font-size: 50px;
            margin: 30px;
        }
        p{
            color: gray;
        
        }
        
        
        .contact{
            display: flex;
            flex-direction: column;
            flex-wrap: nowrap;
            justify-content: center;
            align-items: center;
        }
        
        .input , .big-input{
            width: 200px;
            height: 60px;
            border-radius: 20px;
            text-align: center;
            background-color: rgb(0, 0, 0);
            border: 0.5px solid rgb(126, 126, 126);
            color: white;
            margin: 10px;
            outline: none;
        }
        .two-two{
            display: flex;
            flex-direction: row;
            justify-content: center;
            align-items: center;
            margin: 10px;
        }
        .big-input{
            width: 400px;
        }
        textarea{
            width: 400px;
            height: 250px;
            background-color: black;
            color: white;
            border-radius: 20px;
            background-color: rgb(0, 0, 0);
            border: 0.5px solid rgb(126, 126, 126);
            color: rgb(128, 128, 128);
            margin: 10px;
            outline: none;
            padding: 20px;
        }
        .btn {
            width: 400px;
            height: 60px;
            background-color:rgba(188,8,255,1) ;
            margin-bottom: 50px;
            color: white;
            font-size: 21px;
            margin-top: 10px;
        }
        
        .contact-1 {
            display: flex;
            flex-direction: row;
            flex-wrap: nowrap;
            justify-content: center;
            align-items: center;
        }
        
        .proposal {
            display: flex;
            flex-direction: column;
            flex-wrap: nowrap;
            justify-content: flex-start;
            align-items: flex-start;
            width: 50%;
        
        }
        
        
        @media (max-width:731px) {
            .contact-1{
                flex-direction: column;
            }
            .header{
                flex-direction: column;
            }
            .pic {
                width: 300px;
                height: 300px;
            }
            .name{
                font-size: 30px;
                margin:30px 0px ;
            }
            .main{
                display: none;
            }
            .about-1{
                flex-direction: column;
            }
            .part-1{
                display: flex;
                flex-direction: row;
                flex-wrap: wrap;
                justify-content: center;
                align-items: center;
            }
            .part-2{
                display: flex;
                flex-direction: row;
                flex-wrap: wrap;
                justify-content: center;
                align-items: center;
            }
        }
        .tit{
            text-align: center;
            margin: 20px;
        }
        .data{
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            color: white;
        }
        .link {
            text-decoration: none;
            font-size: 20px;
            color: white;
            margin: 30px 0px;
        }
        .box{
            margin: 20px 0px  ;
            text-align: left;
        }
    </style>
</head>
